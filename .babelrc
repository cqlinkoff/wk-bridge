{
  "env": {
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "loose": true
        }]
      ]
    },
    "test": {
      "presets": [
        ["@babel/preset-env"]
      ]
    },
    "commonjs": {
      "presets": [
        ["@babel/preset-env"]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
